project: spring
applications:
  - name: spring-app
    runtime: ubuntu-jdk8:1.0
    version: "0.1.0"
    dependencies:
      - name: spring-app
        type: debian-package
        origin:
          build: spring-app
        version: 1.0
    command: "java -jar /app/SpringBoot-1.0.0.jar"
  - name: mongo
    runtime: ubuntu-mongo3x:1.0
    dependencies:
      - name: mongo
        type: debian-package
        origin:
          build: mongo
    command: mongo_prov &